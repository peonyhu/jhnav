<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>导航</title>
    <link rel="stylesheet" type="text/css" href="css/common-edc608e548.css"/>
    <link rel="stylesheet" type="text/css" href="css/index-1f3b5c6573.css"/>
</head>
<body>
    <div class="g-header">
        <div id="header">
            <div class="logo">
                <a href="/">
                    <img src="images/logo-3ea90c05ad.png" alt="">
                </a>
            </div>
            <ul class="menu">
                <li class="menu_hover">
                    <a href="/">首页</a> 
                </li>  
                <li>
                    <a href="/addNav">添加导航</a> 
                </li>
            </ul>
            <div class="clear"></div>
        </div>
    </div>
    <div class="g-main-wrap">
        <div class="w1100">
            <h2 class="m-title">常用网站</h2>
            <ul class="m-site-list">
                <% jsonArr.type_id_2.forEach(function(resJson){ %>
                <li><a href="<%= resJson.link %>"><%= resJson.title %></a><a class="u-edit" href="/addNav?id=<%= resJson.id %>">编辑</a><span class="u-del" j-site-id="<%= resJson.id %>">删除</span></li>
                <% }) %>
            </ul>
        
            <h2 class="m-title">内部常用工具</h2>
            <ul class="m-site-list">
                <% jsonArr.type_id_1.forEach(function(resJson){ %>
                    <li><a href="<%= resJson.link %>"><%= resJson.title %></a><a class="u-edit" href="/addNav?id=<%= resJson.id %>">编辑</a><span class="u-del" j-site-id="<%= resJson.id %>">删除</span></li>
                <% }) %>
            </ul>
        </div>
    </div>

    <script type="text/javascript" src="scripts/index-1e92e96447.js"></script>
</body>
</html>